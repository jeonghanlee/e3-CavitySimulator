# Calibration configuration
#

record(ai, $(USER):CavitySimulator:CalButton1){
	field(FLNK, "$(USER):CavitySimulator:CalVM1 PP")
	field(SCAN, "Passive")
}

record(stringout, $(USER):CavitySimulator:CalVM1){
	field(DTYP, "stream")
	field(OUT, "@Calibration.proto calibrate(1) $(PORT)")
	field(SCAN, "Passive")
}

